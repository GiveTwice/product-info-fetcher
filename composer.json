{
    "name": "mattiasgeniar/product-info-fetcher",
    "description": "When given a product URL, will return structured data of that product, including name, description, price, etc",
    "keywords": [
        "mattiasgeniar",
        "product-info-fetcher"
    ],
    "homepage": "https://github.com/mattiasgeniar/product-info-fetcher",
    "license": "MIT",
    "authors": [
        {
            "name": "Mattias Geniar",
            "email": "m@ttias.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4",
        "guzzlehttp/guzzle": "^7.9"
    },
    "require-dev": {
        "pestphp/pest": "^4.0",
        "laravel/pint": "^1.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Mattiasgeniar\\ProductInfoFetcher\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mattiasgeniar\\ProductInfoFetcher\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest --no-coverage",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
